namespace resin "com.caucho.resin";

# configures the default class loader
class-loader {
  compiling-loader "WEB-INF/classes";
  library-loader "WEB-INF/lib";
}

servlet resin-file com.caucho.servlets.FileServlet;

servlet resin-jsp com.caucho.jsp.JspServlet {
  init {
    load-tld-on-init false;
    page-cache-max 1024;
  }
  load-on-startup;
}

servlet resin-jspx com.caucho.jsp.JspServlet {
  init {
    load-tld-on-init false;
    page-cache-max 1024;
    xml true;
  }
  load-on-startup;
}
servlet-mapping url-pattern="*.jsp" servlet-name="resin-jsp" default="true";

servlet-mapping url-pattern="*.jspf" servlet-name="resin-jsp" default="true";

servlet-mapping url-pattern="*.jspx" servlet-name="resin-jspx" default="true";

servlet-mapping url-pattern="/" servlet-name="resin-file" default="true";

servlet "j_security_check" "com.caucho.http.security.FormLoginServlet";

# Configures the special index files to check for directory URLs
welcome-file-list {
  welcome-file index.jsp;
  welcome-file index.html;
}

